{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
      "framework",
      "laravel",
      "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
      "php": ">=7.1.3",
      "ext-json": "*",
      "ext-openssl": "*",
      "ext-pdo": "*"
    },
    "require-dev": {
      "fzaninotto/faker": "~1.4",
      "phpunit/phpunit": "~7.0",
      "barryvdh/laravel-ide-helper": "^2.4",
      "mockery/mockery": "~1.0"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/app/"
      }
    },
    "autoload-dev": {
      "classmap": [
        "tests/",
        "database/"
      ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "optimize-autoloader": true
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://packagist.phpcomposer.com"
      }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "App\\Contents\\ContentsServiceProvider"
            ]
        }
    }
  }
  